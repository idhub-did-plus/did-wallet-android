<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".me.information.UploadInformationTypeActivity">
    <include
        android:id="@+id/title"
        layout="@layout/wallet_common_title_layout"
        android:layout_width="0dp"
        android:layout_height="48dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_upload_idhub_information"
        android:layout_width="286dp"
        android:layout_height="46dp"
        android:background="@drawable/wallet_shape_circle"
        android:gravity="center"
        android:text="@string/wallet_upload_user_information"
        android:textColor="@color/wallet_white"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@id/tv_upload_file"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/title" />

    <TextView
        android:id="@+id/tv_upload_file"
        android:layout_width="286dp"
        android:layout_height="46dp"
        android:background="@drawable/wallet_shape_circle"
        android:gravity="center"
        android:text="@string/wallet_upload_file"
        android:textColor="@color/wallet_white"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_upload_idhub_information" />
</androidx.constraintlayout.widget.ConstraintLayout>