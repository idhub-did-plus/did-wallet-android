            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/third_value"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_12"
            android:layout_marginBottom="@dimen/dp_7"
            android:visibility="gone"
            android:text="+0.65%"
            android:textColor="@color/wallet_white"
            android:textSize="@dimen/font_sp_14"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/fourth_st"
        android:layout_width="@dimen/dp_76"
        android:layout_height="@dimen/dp_0"
        android:layout_marginLeft="@dimen/dp_5"
        android:background="@drawable/wallet_st_shape_fourth"
        app:layout_constraintBottom_toBottomOf="@id/first_st"
        app:layout_constraintLeft_toRightOf="@id/first_st"
        app:layout_constraintTop_toTopOf="@id/first_st">


        <TextView
            android:id="@+id/fourth_name"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_8"
            android:layout_marginTop="@dimen/dp_12"
            android:ellipsize="end"
            android:singleLine="true"
            android:text="Bitcoin"
            android:textColor="@color/wallet_white"
            android:textSize="@dimen/font_sp_16"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/fourth_value"
            android:layout_width="wrap_content"
            android:visibility="gone"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_8"
            android:layout_marginBottom="@dimen/dp_12"
            android:text="+24.18%"
            android:textColor="@color/wallet_white"
            android:textSize="@dimen/font_sp_14"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent" />

        <ImageView
            android:id="@+id/fourth_icon"
            android:layout_width="wrap_content"
